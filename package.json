{
	"name": "gitpruned",
	"displayName": "Git Prun'd",
	"description": "An extension that allows for deleting of multiple local branches.",
	"version": "1.0.3",
	"publisher": "theprodigy161",
	"repository": {
		"url": "https://github.com/TheProdigy161/gitpruned"
	},
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"panel": [
				{
					"id": "git-pruned-explorer",
					"title": "Git Prun'd Explorer",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"git-pruned-explorer": [
				{
					"id": "gitPrunedBranches",
					"name": "Branches",
					"icon": "media/dep.svg"
				}
			]
		},
		"commands": [
			{
				"command": "gitpruned.refreshBranches",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "gitpruned.deleteBranch",
				"title": "Delete",
				"icon": {
					"light": "resources/light/delete.svg",
					"dark": "resources/dark/delete.svg"
				}
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "gitpruned.refreshBranches",
					"when": "view == gitPrunedBranches",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "gitpruned.deleteBranch",
					"when": "view == gitPrunedBranches && viewItem == branch",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
